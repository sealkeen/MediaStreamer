<local:FirstFMPage x:Class="DMultHandler.SignedUpPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DMultHandler"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="SignedUpPage">

    <Grid Background="White">
        <TextBox x:Name="txtLogin" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="24">Successfully signed up as:</TextBox>
    </Grid>
</local:FirstFMPage>

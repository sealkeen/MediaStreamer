<Application x:Class="MediaStreamer.WPF.NetCore3_1.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MediaStreamer.WPF.NetCore3_1"
             StartupUri="MainWindow.xaml"
             Startup="Application_Startup"
             >
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/MediaStreamer.WPF.Resources;component/Resources/Core.xaml"/>
                <ResourceDictionary Source="/MediaStreamer.WPF.Resources;component/Resources/WindowTemplate.xaml"/>
                <ResourceDictionary Source="/MediaStreamer.WPF.Resources;component/Resources/BlankWindowTemplate.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>